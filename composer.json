{
    "name": "sierratecnologia/tools",
    "description": "Recursos para Desenvolvimento Complexo",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "sierratecnologia/casa": "dev-master",
        "sierratecnologia/trainner": "dev-master",
        "sierratecnologia/finder": "dev-master",
        "sierratecnologia/gamer": "dev-master",
        "sierratecnologia/facilitador": "dev-master",
        "sierratecnologia/cms": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Siravel\\": "siravel/",
            "SiUtils\\": "src/utils/",
            "SiHelper\\": "src/helper/",
            "SiLogic\\": "src/logic/",
            "SiObjects\\": "src/objects/",
            "SiPlugins\\": "src/plugins/",
            "SiWeapons\\": "src/recursos/",
            "SiSeed\\": "src/seed/"
        },
        "files": [
            "helpers/functions.php",
            "helpers/helpers.php",
            "helpers/types.php",
            "helpers/general.php",
            "helpers/blade.php",
            "helpers/commerce_helpers.php"
        ]
    },
    "bin": ["bin/code-analyser"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Siravel\\SiravelProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
