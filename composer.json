{
    "name": "sierratecnologia/tools",
    "description": "Recursos para Desenvolvimento Complexo",
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "mockery/mockery": ">=0.9.4",
        "orchestra/testbench": ">=3.6",
        "mikey179/vfsstream": ">=1.6",
        "orchestra/database": ">=3.6",
        "phpunit/phpunit": ">=7.0",
        "orchestra/testbench-browser-kit": ">=3.6",
        "sierratecnologia/facilitador": "dev-master",

        "ricardosierra/phpwhois": "dev-dev",
        "ricardosierra/validate": "^0.2",
        "ricardosierra/translation": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SiUtils\\": "utils/",
            "Siravel\\": "siravel/",
            "SiInteractions\\": "src/interactions/",
            "SiLogic\\": "src/logic/",
            "SiObjects\\": "src/objects/",
            "SiWeapons\\": "src/recursos/",
            "SiSeed\\": "src/seed/"
        },
        "files": [
            "funcs/functions.php",
            "funcs/helpers.php",
            "funcs/types.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../Sitec/Libs/Facilitador",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Siravel\\SiravelProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
