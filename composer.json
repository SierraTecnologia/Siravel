{
    "name": "sierratecnologia/tools",
    "description": "Recursos para Desenvolvimento Complexo",
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "pdepend/pdepend": "*",
        "bitwasp/bitcoin": "*",
        "sierratecnologia/casa": "dev-master",
        "sierratecnologia/trainner": "dev-master",
        "sierratecnologia/finder": "dev-master",
        "sierratecnologia/gamer": "dev-master",
        "sierratecnologia/facilitador": "dev-master",
        "sierratecnologia/cms": "~2.1"
    },
    "require-dev": {
        "mockery/mockery": ">=0.9.4",
        "orchestra/testbench": ">=3.6",
        "mikey179/vfsstream": ">=1.6",
        "orchestra/database": ">=3.6",
        "phpunit/phpunit": ">=7.0",
        "orchestra/testbench-browser-kit": ">=3.6"
    },
    "suggest": {
        "sierratecnologia/payment-laravel": "Pagamento",
        "php-censor/php-censor": "Php Censor"
    },
    "autoload": {
        "psr-4": {
            "Siravel\\": "siravel/",
            "SiUtils\\": "src/utils/",
            "SiHelper\\": "src/helper/",
            "SiLogic\\": "src/logic/",
            "SiObjects\\": "src/objects/",
            "SiPlugins\\": "src/plugins/",
            "SiWeapons\\": "src/recursos/",
            "SiSeed\\": "src/seed/"
        },
        "files": [
            "helpers/functions.php",
            "helpers/helpers.php",
            "helpers/types.php",
            "helpers/general.php",
            "helpers/blade.php",
            "helpers/commerce_helpers.php"
        ]
    },
    "bin": ["bin/code-analyser"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Siravel\\SiravelProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
