{
    "name": "sierratecnologia/tools",
    "description": "Recursos para Desenvolvimento Complexo",
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "mockery/mockery": ">=0.9.4",
        "orchestra/testbench": ">=3.6",
        "mikey179/vfsstream": ">=1.6",
        "sierratecnologia/builder": "dev-master",
        "sierratecnologia/cms": "^2.1",
        "orchestra/database": ">=3.6",
        "phpunit/phpunit": ">=7.0",
        "orchestra/testbench-browser-kit": ">=3.6"
    },
    "autoload": {
        "psr-4": {
            "SiTools\\App\\": "laravel/",
            "SiTools\\Interactions\\": "src/interactions/",
            "SiTools\\Libs\\": "src/libs/",
            "SiTools\\Logic\\": "src/logic/",
            "SiTools\\Objects\\": "src/objects/",
            "SiTools\\Recursos\\": "src/recursos/",
            "SiTools\\Seed\\": "src/seed/"
        },
        "files": [
          "src/libs/helpers.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "SiTools\\App\\SiToolsProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
