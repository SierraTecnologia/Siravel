{
    "name": "sierratecnologia/tools",
    "description": "Recursos para Desenvolvimento Complexo",
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "cloudflare/sdk": "*",
        "deployer/deployer": "^6.4",
        "devio/pipedrive": "*",
        "droath/robo-docker-compose": "*",
        "elasticquent/elasticquent": "*",
        "fideloper/proxy": "^4.0",
        "friendsofphp/php-cs-fixer": "*",
        "fzaninotto/faker": "*",
        "jaeger/querylist": "^4.1",
        "lesstif/php-jira-rest-client": "*",
        "longman/telegram-bot": "*",
        "m4tthumphrey/php-gitlab-api": "*",
        "maknz/slack": "*",
        "markrogoyski/math-php": "*",
        "overtrue/laravel-follow": "^1.1",
        "pdepend/pdepend": "*",
        "php-ai/php-ml": "^0.8.0",
        "php-http/guzzle6-adapter": "*",
        "predis/predis": "*",
        "pusher/pusher-php-server": "~3.4",
        "rap2hpoutre/laravel-log-viewer": "*",
        "rokde/laravel-slow-query-logger": "*",
        "sebastian/diff": "*",
        "sebastian/phpcpd": "*",
        "sensiolabs/security-checker": "*",
        "spatie/crawler": "*",
        "squizlabs/php_codesniffer": "*",
        "srmklive/flysystem-dropbox-v2": "*",
        "symfony/dom-crawler": "*",
        "texthtml/docker-links": "*",
        "waavi/sanitizer": "*",
        "cmgmyr/messenger": "^2.19",
        "sierratecnologia/facilitador": "dev-master",
        "google/photos-library": "^1.4",
        "ricardosierra/phpwhois": "dev-dev",
        "spatie/laravel-backup": "^6.2",
        "zizaco/entrust": "^1.8",
        "raiym/instagram-php-scraper": "^0.8.35",
        "aws/aws-sdk-php": "*",
        "spatie/laravel-activitylog": "^3.6",
        "spatie/laravel-medialibrary": "^7.0.0",
        "graham-campbell/markdown": "*",
        "thujohn/twitter": "^2.2",
        "vanderlee/php-sentence": "^1.0.4",
        "igaster/laravel-model-events": "^1.0.3"
    },
    "require-dev": {
        "mockery/mockery": ">=0.9.4",
        "orchestra/testbench": ">=3.6",
        "mikey179/vfsstream": ">=1.6",
        "orchestra/database": ">=3.6",
        "phpunit/phpunit": ">=7.0",
        "orchestra/testbench-browser-kit": ">=3.6"
    },
    "suggest": {
        "sierratecnologia/payment-laravel": "Pagamento"
    },
    "autoload": {
        "psr-4": {
            "Siravel\\": "siravel/",
            "SiUtils\\": "src/utils/",
            "SiHelper\\": "src/helper/",
            "SiInteractions\\": "src/components/",
            "SiLogic\\": "src/logic/",
            "SiObjects\\": "src/objects/",
            "SiPlugins\\": "src/plugins/",
            "SiWeapons\\": "src/recursos/",
            "SiSeed\\": "src/seed/"
        },
        "files": [
            "funcs/functions.php",
            "funcs/helpers.php",
            "funcs/types.php"
        ]
    },
    "bin": ["bin/code-analyser"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Facilitador",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../Locaravel",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Siravel\\SiravelProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
